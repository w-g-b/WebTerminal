{
  "name": "web-terminal",
  "version": "1.0.0",
  "description": "Web-based terminal with shell command execution",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"node server/app.js\" \"cd client && npm run dev\"",
    "server": "node server/index.js",
    "unified": "node unified-server.js",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "terminal",
    "web",
    "shell",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-pty": "^1.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.3"
  }
}
